{
  "name": "energy-management-system",
  "version": "6.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS=--max-old-space-size=4096 vite",
    "serve": "pnpm dev",
    "build": "rimraf dist && NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:tauri": "rimraf dist && NODE_OPTIONS=--max-old-space-size=8192 vite build --base ./",
    "preview": "vite preview",
    "preview:build": "pnpm build && vite preview",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit --skipLibCheck",
    "svgo": "svgo -f . -r",
    "clean:cache": "rimraf .eslintcache && rimraf pnpm-lock.yaml && rimraf node_modules && pnpm store prune && pnpm install",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,build}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{html,vue,css,scss}\" --ignore-pattern \"dist/**\" --cache-location node_modules/.cache/stylelint/",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [
    "pure-admin-thin",
    "vue-pure-admin",
    "element-plus",
    "tailwindcss",
    "pure-admin",
    "typescript",
    "pinia",
    "vue3",
    "vite",
    "esm"
  ],
  "homepage": "https://github.com/pure-admin/pure-admin-thin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pure-admin/pure-admin-thin.git"
  },
  "bugs": {
    "url": "https://github.com/pure-admin/vue-pure-admin/issues"
  },
  "license": "MIT",
  "author": {
    "name": "xiaoxian521",
    "email": "pureadmin@163.com",
    "url": "https://github.com/xiaoxian521"
  },
  "dependencies": {
    "@iconify-icons/ep": "^1.2.12",
    "@pureadmin/table": "^3.3.0",
    "@pureadmin/utils": "^2.6.2",
    "@tauri-apps/api": "^2.10.1",
    "@vueuse/core": "^14.0.0",
    "@vueuse/motion": "^3.0.3",
    "animate.css": "^4.1.1",
    "dayjs": "^1.11.18",
    "element-plus": "^2.11.5",
    "mitt": "^3.0.1",
    "path-browserify": "^1.0.1",
    "pinia": "^3.0.3",
    "pinyin-pro": "^3.27.0",
    "responsive-storage": "^2.2.0",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vue-tippy": "^6.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@iconify/json": "^2.2.400",
    "@iconify/vue": "4.2.0",
    "@tailwindcss/vite": "^4.1.16",
    "@tauri-apps/cli": "^2.10.0",
    "@types/node": "^20.19.23",
    "@types/path-browserify": "^1.0.3",
    "@types/sortablejs": "^1.15.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "baseline-browser-mapping": "^2.10.0",
    "cssnano": "^7.1.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.5.1",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-load-config": "^6.0.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "stylelint": "^16.25.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-prettier": "^5.0.3",
    "svgo": "^4.0.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "unplugin-icons": "^22.5.0",
    "vite": "^7.1.12",
    "vite-svg-loader": "^5.1.0",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.2"
